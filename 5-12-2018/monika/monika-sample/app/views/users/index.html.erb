<p id="notice"><%= notice %></p>
<h1>My Users</h1>
<table>
<% @users.each do |user|%>
  <tr>
    <td> <b>Name:</b> <%= user.name %> </td>
    <td> <b>Age:</b> <%= user.age %> </td>
    <td><%= link_to 'Show', user_path(user) %></td>
    <td><%= link_to 'Edit', user_edit_path(user) %></td>
    <td><%= link_to 'Delete', user_destroy_path(user), method: :delete %></td>
  </tr>
<% end %>
</table>

<%= link_to 'New', users_new_path %>